{
  "name": "tsstyled",
  "version": "1.2.8",
  "main": "lib/cjs.js",
  "module": "lib/esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs.js",
      "import": "./lib/esm.js"
    }
  },
  "scripts": {
    "start": "webpack serve",
    "test": "npm run eslint && npm run jest",
    "build": "npm run tsc && npm run rollup && npm run check-es-compat",
    "eslint": "eslint src",
    "jest": "del-cli coverage && jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "check-es-compat": "check-es-compat lib/cjs.js",
    "tsc": "del-cli out && tsc --emitDeclarationOnly",
    "rollup": "del-cli lib && rollup -c rollup.config.js",
    "docs": "npm run typedoc && npm run webpack",
    "typedoc": "del-cli docs && make-dir docs && echo 'tsstyled.com' > docs/CNAME && typedoc src/index.ts --out docs --excludePrivate --excludeInternal --excludeExternals --disableOutputCheck --disableSources --theme \"./typedoc-dark\" --highlightTheme dark-plus",
    "webpack": "del-cli docs/benchmark && webpack"
  },
  "keywords": [
    "component",
    "css",
    "css-in-js",
    "react",
    "sass",
    "scss",
    "styled-components",
    "style",
    "styles",
    "styling",
    "theme",
    "typescript",
    "ssr",
    "jss",
    "styled-jsx",
    "emotion",
    "emotionjs"
  ],
  "author": "Chris Ackerman",
  "license": "ISC",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@tinysource/eslint-config": "^1.0.7",
    "@types/benchmark": "^2.1.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "@types/react-is": "^17.0.1",
    "@types/styled-components": "^5.1.11",
    "@types/webpack-env": "^1.16.2",
    "benchmark": "^2.1.4",
    "check-es-compat": "^1.1.1",
    "coveralls": "^3.1.1",
    "css-loader": "^5.2.6",
    "d3-scale-chromatic": "^3.0.0",
    "del-cli": "^4.0.0",
    "eslint": "^7.29.0",
    "html-webpack-plugin": "^5.3.2",
    "iconoir-react": "^2.1.0",
    "jest": "^27.0.6",
    "make-dir-cli": "^3.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.3.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.2",
    "rollup": "^2.52.7",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "style-loader": "^3.0.0",
    "styled-components": "^5.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.2",
    "typescript": "4.2.4",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3",
    "zod": "^3.5.1"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@tinysource"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/tsstyled.git"
  },
  "bugs": {
    "url": "https://github.com/Shakeskeyboarde/tsstyled/issues"
  },
  "homepage": "https://tsstyled.com",
  "browserslist": [
    "IE >= 11"
  ]
}
