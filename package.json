{
  "name": "tsstyled",
  "version": "1.1.2",
  "main": "lib/cjs.js",
  "module": "lib/esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs.js",
      "import": "./lib/esm.js"
    }
  },
  "scripts": {
    "test": "npm run build && npm run eslint && npm run jest",
    "build": "npm run tsc && npm run rollup && npm run check-es-compat && npm run typedoc",
    "prepack": "npm i && npm test",
    "eslint": "eslint src",
    "jest": "del-cli coverage && jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "check-es-compat": "check-es-compat lib/cjs.js",
    "tsc": "del-cli out && tsc",
    "rollup": "del-cli lib && rollup -c rollup.config.js",
    "typedoc": "del-cli \"docs/*\" \"!docs/CNAME\" && typedoc src/index.ts --out docs --excludePrivate --excludeInternal --excludeExternals --disableOutputCheck --disableSources --theme \"./typedoc-dark\" --highlightTheme dark-plus"
  },
  "keywords": [
    "component",
    "css",
    "css-in-js",
    "react",
    "sass",
    "scss",
    "styled-components",
    "styles",
    "styling",
    "theme",
    "typescript"
  ],
  "author": "Chris Ackerman",
  "license": "ISC",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@tinysource/eslint-config": "^1.0.4",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "check-es-compat": "^1.1.1",
    "coveralls": "^3.1.0",
    "del-cli": "^4.0.0",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.52.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@tinysource"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/tsstyled.git"
  },
  "bugs": {
    "url": "https://github.com/Shakeskeyboarde/tsstyled/issues"
  },
  "homepage": "https://tsstyled.com",
  "browserslist": [
    "IE >= 11"
  ]
}
