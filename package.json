{
  "name": "tsstyled",
  "version": "3.0.5",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "start": "webpack serve",
    "test": "tsc --noEmit && eslint src && del-cli coverage && jest",
    "build": "del-cli lib && tsc -p tsconfig-types.json && tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json && check-es-compat lib/cjs",
    "docs": "del-cli \"docs/*\" \"!docs/CNAME\" && typedoc src/index.ts --out docs --disableSources --cleanOutputDir false && webpack",
    "coveralls": "cat ./out/coverage/lcov.info | coveralls"
  },
  "keywords": [
    "component",
    "css",
    "css-in-js",
    "react",
    "sass",
    "scss",
    "styled-components",
    "style",
    "styles",
    "styling",
    "theme",
    "typescript",
    "ssr",
    "jss",
    "styled-jsx",
    "emotion",
    "emotionjs"
  ],
  "author": "Chris Ackerman",
  "license": "ISC",
  "files": [
    "lib/**"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "@types/react-is": "^17.0.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "check-es-compat": "^2.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.1",
    "d3-scale-chromatic": "^3.0.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-feather": "^2.0.9",
    "react-is": "^18.0.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typedoc": "^0.22.14",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/tsstyled.git"
  },
  "bugs": {
    "url": "https://github.com/Shakeskeyboarde/tsstyled/issues"
  },
  "homepage": "https://tsstyled.com",
  "browserslist": [
    "IE >= 11"
  ]
}
