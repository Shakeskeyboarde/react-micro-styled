{
  "name": "tsstyled",
  "version": "2.0.0",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "del-cli out lib docs",
    "start": "webpack serve",
    "test": "tsc --noEmit && eslint src && del-cli coverage && jest",
    "build": "del-cli out lib && tsc --noEmit false --emitDeclarationOnly --declaration --outDir out && cross-env NODE_ENV=production rollup -c rollup.config.js && check-es-compat lib/cjs/index.js",
    "docs": "del-cli docs && make-dir docs && echo 'tsstyled.com' > docs/CNAME && typedoc src/index.ts --out docs --excludePrivate --excludeInternal --excludeExternals --cleanOutputDir --disableSources && webpack",
    "coveralls": "cat ./out/coverage/lcov.info | coveralls"
  },
  "keywords": [
    "component",
    "css",
    "css-in-js",
    "react",
    "sass",
    "scss",
    "styled-components",
    "style",
    "styles",
    "styling",
    "theme",
    "typescript",
    "ssr",
    "jss",
    "styled-jsx",
    "emotion",
    "emotionjs"
  ],
  "author": "Chris Ackerman",
  "license": "ISC",
  "files": [
    "lib/**"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@types/benchmark": "^2.1.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "@types/react-is": "^17.0.2",
    "@types/styled-components": "^5.1.14",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "benchmark": "^2.1.4",
    "check-es-compat": "^1.1.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "d3-scale-chromatic": "^3.0.0",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "iconoir-react": "^2.1.0",
    "jest": "^27.2.2",
    "make-dir-cli": "^3.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "style-loader": "^3.3.0",
    "styled-components": "^5.3.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "zod": "^3.9.1"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/tsstyled.git"
  },
  "bugs": {
    "url": "https://github.com/Shakeskeyboarde/tsstyled/issues"
  },
  "homepage": "https://tsstyled.com",
  "browserslist": [
    "IE >= 11"
  ]
}
