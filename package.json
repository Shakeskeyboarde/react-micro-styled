{
  "name": "tsstyled",
  "description": "A small, fast, and simple CSS-in-JS styled components solution for React, written in Typescript.",
  "author": {
    "name": "Chris Ackerman",
    "email": "chris@topher.land"
  },
  "license": "ISC",
  "version": "3.0.11",
  "files": [
    "lib/**"
  ],
  "directories": {
    "lib": "lib"
  },
  "types": "lib/types/index.d.ts",
  "module": "lib/module/index.js",
  "main": "lib/commonjs/index.js",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/module/index.js",
    "default": "./lib/commonjs/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "del-cli out && tsc --noEmit && eslint --max-warnings=0 src && jest --no-cache --passWithNoTests",
    "prepublishOnly": "npm run build",
    "build": "del-cli lib && tsc --project tsconfig.build-commonjs.json && check-es-compat lib/commonjs && tsc --project tsconfig.build-module.json && tsc --project tsconfig.build-types.json",
    "start": "webpack serve",
    "modernize": "npm-check-updates -u -x react,react-dom,@testing-library/react",
    "docs": "del-cli \"docs/*\" \"!docs/CNAME\" && typedoc src/index.ts --out docs --disableSources --cleanOutputDir false && webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-is": "^17.0.3",
    "@types/styled-components": "^5.1.25",
    "babel-loader": "^8.2.5",
    "check-es-compat": "^2.1.0",
    "core-js": "^3.23.5",
    "css-loader": "^6.7.1",
    "d3-scale-chromatic": "^3.0.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.20.0",
    "eslint-config-rational": "^0.1.16",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "normalize.css": "^8.0.1",
    "npm-check-updates": "^15.3.4",
    "prettier": "^2.7.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-feather": "^2.0.10",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.23.8",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">=16.14.0"
  },
  "keywords": [
    "component",
    "css",
    "css-in-js",
    "react",
    "sass",
    "scss",
    "styled-components",
    "style",
    "styles",
    "styling",
    "theme",
    "typescript",
    "ssr",
    "jss",
    "styled-jsx",
    "emotion",
    "emotionjs"
  ]
}
